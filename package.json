{
  "name": "table-topicker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx,css}\" --cache",
    "check:format": "prettier --check ./src",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "check:build": "next build",
    "test": "vitest run",
    "check": "pnpm run check:format && pnpm run check:types && pnpm run check:lint && pnpm run test && pnpm run check:build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:clear": "tsx src/scripts/clear-database.ts",
    "db:seed": "tsx src/scripts/seed-base-questions.ts",
    "db:fresh": "pnpm run db:clear && pnpm run db:migrate && pnpm run db:seed",
    "db:production:migrate": "tsx src/scripts/migrate-production.ts"
  },
